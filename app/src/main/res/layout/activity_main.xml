            android:layout_height="wrap_content"
            android:text="@string/multimedia_audio"
        />

        <Button
            android:id="@+id/btn_three"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/multimedia_video_use_surfaceview_preview_video"
